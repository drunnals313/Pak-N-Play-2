<main id="barba-wrapper">
	<div class="barba-container" data-namespace="AllListings">
		

				<div class="panel panel-primary">
					<div class="panel-heading">
						<h3 class="panel-title">
							<strong>{{_doc.item_name}}</strong>
						</h3>
					</div>
					<div class="panel-body">

						<div class="row">
							<div class="col-xs-7">${{_doc.price}}</div>
							<div class="col-xs-5">
								<div class="pull-right">
									<button class="change-purchase" data-id="{{_doc._id}}" data-newpurchase="{{purchase}}">
										{{#if purchase}}Rent Now!{{else}}Return Now!{{/if}}
									</button>

									<button class="delete-item" data-id="{{_doc._id}}">Delete!</button>
								</div>
							</div>
						</div>
					</div>
				</div>
			
	</div>
</main>

<script src="https://cdnjs.cloudflare.com/ajax/libs/barba.js/1.0.0/barba.js" type="text/javascript"></script>